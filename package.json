{
  "name": "morse-code-kata",
  "version": "1.0.0",
  "description": "morse code kata",
  "main": "index.js",
  "scripts": {
    "build": "babel source --out-dir ./target --source-maps",
    "debug": "node --inspect-brk ./target/index.js",
    "serve": "node ./target/index.js",
    "start": "nodemon --exec babel-node ./source/index.js",
    "test": "mocha --require @babel/register",
    "test-auto": "mocha --require @babel/register -w --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raydecastro/morse-code-kata.git"
  },
  "keywords": [
    "morse",
    "code",
    "kata"
  ],
  "author": "ray castro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raydecastro/morse-code-kata/issues"
  },
  "homepage": "https://github.com/raydecastro/morse-code-kata#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6"
  }
}
